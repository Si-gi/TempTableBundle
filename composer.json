{
    "name": "sigi/temp-table-bundle",
    "description" : "Symfony bundle to create temp database",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Simon gilbert",
            "email": "gilbert.simonj@gmail.com"
        }
    ],
    "prefer-stable": true,
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/console": "7.3.*",
        "symfony/dotenv": "7.3.*",
        "symfony/runtime": "7.3.*",
        "symfony/yaml": "7.3.*",
        "doctrine/dbal": "3.10.*"

    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true,
            "phpro/grumphp": true,
            "symfony/flex": true,
            "symfony/runtime": true
        },
        "bump-after-update": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sigi\\TempTableBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sigi\\Tests\\TempTableBundle\\": "tests/"
        }
    },
    "scripts": {
        "auto-scripts": {
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "7.3.*"
        }
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpro/grumphp": "*",
        "phpstan/phpstan": "*",
        "phpspec/prophecy": "~1.0",
        "phpspec/prophecy-phpunit": "^2.4",
        "mikey179/vfsstream": "1.6.12"
    }
}
